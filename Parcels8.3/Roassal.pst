ect: [ :n | (n getShape: ROBox) borderColorFor: n]) = #(3 4 5)</body><body package="Roassal-Core">testBorderColorWithNormalizer	view shape rectangle borderColor: (RONIdentityNormalizer new).	view nodes: (1 to: 10).	self shouldnt: [view raw drawOn: RONullCanvas new]  raise: Error</body><body package="Roassal-Core">testBorderWidth	| nodes b |	view := ROMondrianViewBuilder new.	view shape rectangle borderWidth: (b := [ :model | model + 1 ]).	nodes := view nodes: #(2 3 4).		self assert: (nodes collect: [ :n | (n getShape: ROBox) borderWidthFor: n]) = #(3 4 5)</body><body package="Roassal-Core">testFillColor	| nodes |	view shape rectangle fillColor: [ :model | model + 1].	nodes := view nodes: #(2 3 4).		self assert: (nodes collect: [ :n | (n getShape: ROBox) colorFor: n]) = #(3 4 5)</body><body package="Roassal-Core">testHeight	| nodes heightBlock shape |	heightBlock := [ :model | model + 1 ].	view shape rectangle height: heightBlock.	"self assert: view height == heightBlock."	nodes := view nodes: #(2 30 40).		shape := nodes first getShape: ROBox.	self assert: (shape height roValue: nodes first) = 3.	self assert: (shape height roValue: nodes second) = 31.	self assert: (nodes collect: [ :n | n height ]) =  #(5 31 41)</body><body package="Roassal-Core">testIfBorderColor	| nodes |	view shape rectangle 		if: #odd borderColor: [ :model | model + 1];		if: #even borderColor: [ :model | model + 10].	nodes := view nodes: #(2 3 4 5 6).		self assert: (nodes collect: [ :n | (n getShape: ROBox) borderColorFor: n]) = #(12 4 14 6 16)</body><body package="Roassal-Core">testIfFillColor	| nodes |	view shape rectangle 		if: #odd fillColor: [ :model | model + 1];		if: #even fillColor: [ :model | model + 10].	nodes := view nodes: #(2 3 4 5 6).		self assert: (nodes collect: [ :n | (n getShape: ROBox) colorFor: n]) = #(12 4 14 6 16)</body><body package="Roassal-Core">testLogHeight	| nodes |	view shape rectangle logHeight: [ :model | 